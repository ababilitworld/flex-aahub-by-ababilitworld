    "homepage": "https://ababilithub.com/wp-plugin/flex-aahub-by-ababilithub",
    "license": "GPL-3.0-or-later",
    "license_uri": "https://www.gnu.org/licenses/gpl-3.0.html",
    "authors": [
        {
            "name": "Ababil IT World",
            "email": "ababilithub@gmail.com",
            "homepage": "https://ababilithub.com/"
        },
        {
            "name": "Md Shafiul Alam",
            "email": "cse.shafiul@gmail.com",
            "homepage": "https://ababilithub.com/"
        }
    ],
    "require": {
        "firebase/php-jwt": "^6.0",
        "ababilithub/flex-php": "dev-main",
        "ababilithub/flex-wordpress": "dev-main"       
    },
    "autoload": {
        "psr-4": {
            "Ababilithub\\FlexAahub\\Package\\": "src/Package"
        },
        "files": [
            "includes.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ababilithub/flex-aahub-by-ababilithub"
        }
    ],
    "extra": { 
        "wordpress-plugin": {
            "name": "Flex Aahub By Ababil IT World",
            "uri": "https://ababilithub.com/wp-plugin/flex-aahub-by-ababilithub",
            "description": "The Ababilithub Plugins's Aahub Funtionalities, Exclusively by Ababil IT World.",
            "version": "1.0.0",
            "requires": {
                "firebase/php-jwt": "^6.0",
                "ababilithub/flex-trait-by-ababilithub": "dev-main",
                "ababilithub/flex-pagination-by-ababilithub": "dev-main",
                "ababilithub/flex-theme-info-by-ababilithub": "dev-main"
            },
            "tested": "6.5",
            "author": "Ababil IT World",
            "author_uri": "https://ababilithub.com/",
            "author_email": "ababilithub@gmail.com",
            "license": "GPL-3.0-or-later",
            "text_domain": "flex-aahub-by-ababilithub",
            "domain_path": "/language",
            "namespace": "Ababilithub\\FlexAahub",
            "prefix": "Ababilithub\\FlexAahub\\PLUGIN_DIR"
        }
    }
}
